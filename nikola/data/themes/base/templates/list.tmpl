## -*- coding: utf-8 -*-
<%inherit file="base.tmpl"/>
<%block name="content">
        <article class="postbox">
        <header>
            <h1>${title}</h1>
        </header>
        %if items:
        <ul>
        % for text, link in items:
            <li><a href="${link}">${text}</a>
        % endfor
        </ul>
        %endif
        </article>
</%block>
