## -*- coding: utf-8 -*-
<!-- Begin post-list ${post_list_id} -->
<div id="${post_list_id}" class="post-list">
    <ul class="post-list">
        % for post in posts:
            <li>
	        <span class="post-list-item">
		${post.formatted_date(date_format)}</span>
		&nbsp;<a href="${post.permalink(lang)}">${post.title(lang)}</a>
	    </li>
        % endfor
    </ul>
</div>
<!-- End post-list ${post_list_id} -->
